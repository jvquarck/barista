{
  "$schema": "http://json-schema.org/schema",
  "id": "SchemanticsForDtLintingRule",
  "title": "DtLintingRule Schema",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "alttext": {
      "type": "string"
    },
    "severity": {
      "type": "string",
      "enum": ["warning", "error"],
      "default": "error"
    }
  }
}
