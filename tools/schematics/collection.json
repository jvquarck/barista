{
  "$schema": "../../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "dt-component": {
      "description": "Create a component with a demo",
      "factory": "./dt-component/index",
      "schema": "./dt-component/schema.json",
      "aliases": ["dtc"]
    },
    "dt-example": {
      "description": "Create a new example in docs component",
      "factory": "./dt-example/index",
      "schema": "./dt-example/schema.json",
      "aliases": ["dte"]
    },
    "dt-demo": {
      "description": "Create a new demo in dev-app",
      "factory": "./dt-demo/index",
      "schema": "./dt-demo/schema.json",
      "aliases": ["dtd"]
    },
    "dt-linting-rule": {
      "description": "Create a new linting rule",
      "factory": "./dt-linting-rule/index",
      "schema": "./dt-linting-rule/schema.json",
      "aliases": ["dtl"]
    }
  }
}
