{
  "name": "@dynatrace/angular-components",
  "version": "4.14.1",
  "description": "",
  "scripts": {
    "barista-examples": "gulp barista-example:generate && ng serve barista-examples",
    "barista-examples:build": "gulp barista-examples:build",
    "ide-completions": "gulp ide-completions",
    "dev": "ng serve dev-app",
    "barista": "gulp barista:generate-content && ng serve barista",
    "build": "gulp library:build && gulp ts-linting-rules:build",
    "stage-release": "ts-node --project tools/release/tsconfig.json tools/release/stage-release.ts",
    "get-release-version": "ts-node --project tools/release/tsconfig.json tools/release/get-release-version.ts",
    "create-tag": "ts-node --project tools/release/tsconfig.json tools/release/create-tag.ts",
    "breaking-changes": "gulp breaking-changes",
    "test": "ng test lib",
    "test:watch": "ng test lib --watch",
    "ui-test": "ng e2e --project ui-test-app",
    "ui-test:watch": "watch 'ng e2e --project ui-test-app --devServerTarget' src/lib src/ui-test-app ui-tests --wait=1",
    "a11y-test": "ng test --project=a11y-test-app",
    "lint": "gulp lint",
    "tslint:build": "gulp ts-linting-rules:build",
    "tslint:test": "tslint --rules-dir dist/lib/tslint/rules --test src/linting/test/rules/**/tslint.json",
    "stylelint-check": "stylelint-config-prettier-check",
    "prettier": "prettier \"./**/*.{ts,js,css,scss,json,html,md}\"",
    "prettier:fix": "yarn prettier --write",
    "import-sort": "import-sort \"src/!(schematics)/**/*.{ts,js}\" \"tools/tslint-rules/**/*.{ts,js}\" \"ui-tests/**/*.{ts,js}\"",
    "import-sort:fix": "yarn import-sort --write",
    "import-sort-style-dynatrace:build": "tsc -b tools/import-sort-style-dynatrace && copyfiles -u 2 \"tools/import-sort-style-dynatrace/*.json\" node_modules/import-sort-style-dynatrace",
    "universal": "gulp universal",
    "schematics:build": "gulp schematics:build",
    "postinstall": "gulp schematics:build && gulp barista-example:generate && yarn import-sort-style-dynatrace:build",
    "bitbucket-tools": "nodemon --config ./tools/bitbucket-tools/nodemon.json",
    "bitbucket-tools:test": "jest --rootDir tools/bitbucket-tools/",
    "bitbucket-tools:build": "tsc --project ./tools/bitbucket-tools/tsconfig.json"
  },
  "author": "Dynatrace",
  "license": "SEE LICENSE IN LICENSE",
  "browserslist": [
    "last 1 year",
    "Firefox ESR",
    "safari >= 10"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "jestSonar": {
    "reportPath": "dist/sonar-test-results/",
    "reportFile": "unit-tests.xml"
  },
  "importSort": {
    ".ts": {
      "parser": "typescript",
      "style": "dynatrace"
    },
    ".js": {
      "parser": "babylon",
      "style": "dynatrace"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "import-sort --write",
      "prettier --write",
      "git add"
    ],
    "*.{scss,css,html,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "core-js": "^3.1.3",
    "d3-scale": "^3.0.0",
    "d3-shape": "^1.3.5",
    "lodash": "^4.17.11",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.3",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "^0.800.2",
    "@angular-devkit/core": "^8.0.2",
    "@angular-devkit/schematics": "^8.0.2",
    "@angular/cli": "^8.0.2",
    "@angular/compiler": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/platform-server": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@dynatrace/angular-lint-rules": "^0.6.0",
    "@dynatrace/dt-iconpack": "^2.0.22",
    "@nrwl/jest": "^8.2.0",
    "@nrwl/workspace": "^8.2.0",
    "@types/chalk": "^0.4.31",
    "@types/cross-spawn": "^6.0.0",
    "@types/d3-scale": "^2.1.1",
    "@types/d3-shape": "^1.3.1",
    "@types/fs-extra": "^5.0.0",
    "@types/glob": "^5.0.33",
    "@types/gulp": "^4.0.6",
    "@types/highcharts": "^5.0.20",
    "@types/inquirer": "^0.0.43",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/markdown-it": "^0.0.9",
    "@types/sass-graph": "^2.1.2",
    "@types/stylelint": "^9.10.0",
    "@types/through2": "^2.0.33",
    "axe-webdriverjs": "^2.3.0",
    "axios": "^0.19.0",
    "chalk": "^1.1.3",
    "codelyzer": "^5.1.0",
    "compare-func": "^1.3.2",
    "conventional-changelog": "^3.0.5",
    "conventional-recommended-bump": "^4.0.4",
    "copyfiles": "^2.1.1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^8.0.1",
    "gray-matter": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-util": "^3.0.8",
    "highcharts": "^6.0.7",
    "husky": "^3.0.0",
    "import-sort": "^6.0.0",
    "import-sort-cli": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "inquirer": "^6.2.1",
    "jasmine-reporters": "^2.3.2",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "jest-preset-angular": "^7.1.1",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^9.2.1",
    "markdown-it": "^10.0.0",
    "merge2": "^1.2.3",
    "minimatch": "^3.0.4",
    "ng-packagr": "^5.3.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "protractor": "^5.4.1",
    "puppeteer": "^1.17.0",
    "q": "^1.5.1",
    "resolve-bin": "^0.4.0",
    "rxjs-tslint": "0.1.5",
    "sass-graph": "^3.0.4",
    "stylelint": "^10.1.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "stylelint-scss": "^3.9.1",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^2.3.0",
    "tsickle": "^0.35.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "~3.4.3",
    "typescript-tslint-plugin": "^0.5.0",
    "undertaker-forward-reference": "^1.0.2",
    "watch": "^1.0.2",
    "web-animations-js": "^2.3.1"
  }
}
